{
  "name": "content-tag",
  "description": "A rust program that uses a fork of swc to parse and transform Javascript containing the content-tag proposal",
  "version": "1.1.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/embroider-build/content-tag"
  },
  "files": [
    "index.d.ts",
    "index.d.cts",
    "standalone.js",
    "standalone/*",
    "node/*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "import": "./standalone.js"
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./pkg/node/content_tag.cjs"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./pkg/node/content_tag.cjs"
      }
    }
  }
}
